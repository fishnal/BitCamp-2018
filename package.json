{
  "name": "public-spotify-queue",
  "description": "Allows Spotify users to broadcast not only their current listening song, but as well as their queues",
  "contributors": [
    "Vishal Patel",
    "Brian Park"
  ],
  "license": "MIT",
  "version": "0.1.1",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "body-parser": "^1.18.3",
    "commander": "^2.19.0",
    "express": "^4.16.3",
    "humps": "^2.0.1",
    "js-cookie": "^2.2.0",
    "mochawesome": "^3.0.3",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "spotify-web-api-node": "^4.0.0",
    "sprintf-js": "^1.1.1"
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "prebuild": "gulp makeBuildDir",
    "build:js": "babel src/js --out-dir ./build/static/js --source-maps",
    "build": "npm run build:js",
    "prestart": "npm run build",
    "start": "node ./build/static/js/index.js --config production.conf",
    "test": "gulp test"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.2.0",
    "gulp": "^4.0.0",
    "gulp-mocha": "^6.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "react-dom": "^16.7.0",
    "should": "^13.2.3"
  },
  "apidoc": {
    "name": "Public Spotify Queue",
    "description": "REST API endpoints for Public Spotify Queue",
    "title": "Public Spotify Queue API",
    "version": "0.1.1",
    "order": [
      "Client",
      "GetHome",
      "GetIndex",
      "SpotifyAuth",
      "GetTokens",
      "RefreshToken",
      "Queue",
      "AddAfter",
      "RemoveSong"
    ]
  }
}
